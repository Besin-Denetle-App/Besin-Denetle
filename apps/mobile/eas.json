{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "credentialsSource": "local",
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "preview",
        "API_HOST": "192.168.1.106",
        "API_PORT": "3200"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "production",
        "API_URL": "https://besindenetle.furkanpasa.com/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
