{
  "name": "@besin-denetle/backend",
  "version": "0.19.0",
  "description": "RESTful API built with NestJS for Besin Denetle food analysis platform",
  "author": "Furkan Ã‡elik <iletisim.furkancelik@gmail.com> (https://furkanpasa.com)",
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Besin-Denetle-App/Besin-Denetle.git",
    "directory": "apps/backend"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node apps/backend/dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "db:generate": "npx ts-node src/scripts/generate-migration.ts",
    "db:migrate": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "db:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "db:show": "typeorm-ts-node-commonjs migration:show -d src/data-source.ts",
    "analyze": "npx ts-node --transpile-only src/scripts/analyze-csv.ts",
    "importcsv": "npx ts-node --transpile-only src/scripts/import-csv.ts",
    "recalculate": "npx ts-node --transpile-only src/scripts/recalculate-scores.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "@besin-denetle/shared": "workspace:*",
    "@google/genai": "^1.35.0",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "google-auth-library": "^10.5.0",
    "ioredis": "^5.9.2",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.28",
    "uuid": "^11.1.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}